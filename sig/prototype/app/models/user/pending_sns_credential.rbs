class User::PendingSnsCredential < ApplicationRecord
  # トークンの有効期限（デフォルト10分）
  TOKEN_EXPIRATION_TIME: untyped

  # トークンから有効なレコードを検索
  def self.find_valid_by_token: (untyped token) -> untyped

  # OmniAuthDataから作成
  def self.create_from_omniauth!: (untyped omniauth_data) -> untyped

  # 期限切れかどうか
  def expired?: () -> untyped

  # OmniauthDataオブジェクトに変換
  def to_omniauth_data: () -> untyped

  private

  def self.generate_secure_token: () -> untyped
end
